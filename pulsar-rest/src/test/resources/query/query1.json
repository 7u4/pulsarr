{
  "parse": true,
  "expires": "30m",
  "withEntity": true,
  "linkFilter": {
    "minAnchorLen": 2,
    "manAnchorLen": 200,
    "minUrlLen": 35,
    "maxUrlLen": 200
  },
  "projection": {
    "title": "#title",
    "html_content": "outerHtml(`#title`)",
    "text_content": "text(`#title`)"
  }
}
